<section class="main">
  <div class='row linktoportal'>
    <div class="col-sm-12">
      <a href="adminportal">
        <button>Return to Admin Portal</button>
      </a>
    </div>
  </div>
  <div class='row'>
    <div class="col-sm-12">
          <h3>Message Activity</h3>
    </div>
  </div>
  <div class="messageDisplay">
    <div class="row">
      <div class="col-xs-1">
      </div>
      <div class="col-xs-2">
        <h3>Date</h3>
      </div>
      <div class="col-xs-2">
        <h3>Name</h3>
      </div>
      <div class="col-xs-2">
        <h3>E-Mail</h3>
      </div>
      <div class="col-xs-5">
        <h3>Message</h3>
      </div>
    </div>

    {{#each messagedata}}
      <hr>
      <div class="row">
        <div class="col-xs-1 deleteColumn">
          <a href="deletemessage/{{this.id}}">
            <i class="fa fa-times-circle" aria-hidden="true"></i>
          </a>
        </div>
        <div class="col-xs-2">
          {{this.createdAt}}
        </div>
        <div class="col-xs-2">
          {{this.name}}
        </div>
        <div class="col-xs-2">
          {{this.email}}
        </div>
        <div class="col-xs-5">
          {{this.message}}
        </div>
      </div>
    {{/each}}
  </div>
</section>